#include <iostream>
using namespace std;

int main () {
	//membuat program break
	
	cout << "Nama : Muhammad Hilal Zada" <<endl;
	cout << "NIM  : ETE10190103" <<endl;
	cout << "Prodi: Teknik Elektromedik" <<endl;
	cout << endl;
	cout << "Berikut adalah tampilan dari program break" <<endl;
	for (int i = 10 ; i <= 50 ; i++) {
		if (i > 50) break;
		cout << i << endl;
	}
}
